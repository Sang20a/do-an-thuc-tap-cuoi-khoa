<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".goicuoc_Fragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="54dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="55dp"
            android:text="DANH SÁCH GÓI CƯỚC"
            android:textColor="@color/blue"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ListView
            android:id="@+id/lvgc"
            android:layout_width="409dp"
            android:layout_height="500dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Spinner
            android:id="@+id/spntheongay"
            android:layout_width="120dp"
            android:layout_height="48dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="17dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/lvgc"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <Spinner
            android:id="@+id/spntheogia"
            android:layout_width="120dp"
            android:layout_height="48dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="17dp"
            android:layout_marginBottom="15dp"
            app:layout_constraintBottom_toTopOf="@+id/lvgc"
            app:layout_constraintStart_toEndOf="@+id/spntheongay"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>